{
  "name": "context-easy",
  "version": "1.3.1",
  "description": "The easiest way to manage state in a React application!",
  "keywords": [
    "React",
    "state",
    "context",
    "hooks"
  ],
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvolkmann/context-easy.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "babel src -d lib",
    "format": "prettier --write 'src/**/*.{js,css}'",
    "lint": "eslint --quiet src --ext .js",
    "prepublish": "babel src -d lib && cp src/*.css lib",
    "prepush": "npm run verify",
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "test": "npm run build && jest",
    "verify": "npm-run-all format lint test"
  },
  "browserslist": [
    ">0.25%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "verbose": true
  },
  "dependencies": {
    "lodash": "^4.17.14",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/react": "^8.0.5",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.8.6"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
