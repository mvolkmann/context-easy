{
  "name": "context-easy",
  "version": "0.4.3",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src -d lib",
    "format": "prettier --write 'src/**/*.{js,scss}'",
    "lint": "eslint --quiet src --ext .js",
    "prepush": "npm run verify",
    "test": "react-scripts test",
    "test-no-watch": "cross-env CI=true react-scripts test --env=jsdom",
    "verify": "npm-run-all format lint test-no-watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "lodash": "^4.17.11",
    "react": "16.7.0-alpha.0",
    "react-dom": "16.7.0-alpha.0",
    "react-scripts": "2.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0",
    "eslint-plugin-html": "^5.0.0",
    "husky": "^1.1.4",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.15.2",
    "react-testing-library": "^5.2.3"
  }
}
