{
  "name": "context-easy",
  "version": "0.10.3",
  "description": "The easiest way to manage state in a React application!",
  "keywords": [
    "React",
    "state",
    "context",
    "hooks"
  ],
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvolkmann/context-easy.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "babel src -d lib",
    "format": "prettier --write 'src/**/*.{js,scss}'",
    "lint": "eslint --quiet src --ext .js",
    "prepublish": "babel src -d lib",
    "prepush": "npm run verify",
    "test": "jest",
    "verify": "npm-run-all format lint test"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "react-testing-library": "^5.5.4"
  },
  "peerDependencies": {
    "react": "16.8.1",
    "react-dom": "16.8.1"
  }
}
